{
  "baseURI": "/html/en/default/rest/Equilibrium/",
  "dispatchers": [
    {
      "resourceURI": "app/.*",
      "dispatcher": "in.vadlakonda.equilibrium.dispatch.ResourceDispatcher",
      "resourceRoot": "main/"
    },
    {
      "resourceURI": "api/.*",
      "dispatcher": "in.vadlakonda.equilibrium.dispatch.APIDispatcher",
      "configFile": "api-config.json"
    },
    {
      "resourceURI": "ERROR_RESPONSE",
      "dispatcher": "in.vadlakonda.equilibrium.dispatch.ErrorResponseDispatcher"
    }
  ]
}